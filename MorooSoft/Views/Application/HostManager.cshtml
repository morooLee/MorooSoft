@{
    ViewBag.Title = "| Host Manager";
}
@model string

<div class="moroo-main">
    <div class="moroo-app-content">
        <div class="row">
            <div class="col-md-6 moroo-padding-0">
                <h3 class="moroo-margin-0">
                    Host Manager
                </h3>
            </div>
            <div class="col-md-6 moroo-padding-0">
                <h3 class="moroo-margin-0">
                    (@ViewBag.Message)
                </h3>
            </div>
        </div>
        <div class="row moroo-app-content moroo-margin-top-10">
            <div class="col-xs-3"></div>
            <div class="col-xs-3 moroo-padding-5">
                <a class="btn btn-primary" href="http://www.moroosoft.com/files/hostmanager/32bit/hostmanager.exe">32 Bit</a>
            </div>
            <div class="col-xs-3 moroo-padding-5">
                <a class="btn btn-primary" href="http://www.moroosoft.com/files/hostmanager/64bit/hostmanager.exe">64 Bit</a>
            </div>
            <div class="col-xs-3"></div>
        </div>
    </div>
</div>